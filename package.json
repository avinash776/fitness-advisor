{
  "name": "fitness-advisor",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "A modern web application that provides personalized fitness advice using AI. Built with React, TypeScript, and Tailwind CSS.",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:proxy": "node proxy-server.mjs",
    "dev:full": "start cmd /c \"npm run dev:proxy\" && npm run dev",
    "dev:netlify": "netlify dev",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "vite preview --host 0.0.0.0 --port 8080",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@types/node": "^20.11.19",
    "aos": "^2.3.4",
    "chart.js": "^4.4.1",
    "jspdf": "^3.0.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@types/aos": "^3.0.4",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "cors": "^2.8.5",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^5.2.1",
    "netlify-cli": "^17.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
  },
  "keywords": [
    "fitness",
    "react",
    "typescript",
    "tailwind",
    "ai"
  ],
  "author": "Avinash A",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/YOUR_USERNAME/fitness-advisor.git"
